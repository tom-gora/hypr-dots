{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "closer": "#92638E",
    "orange": "#62496B",
    "os": "#827A9E",
    "purple": "#2F2751",
    "transient": "#383838",
    "user": "p:os"
  },
  "transient_prompt": {
    "template": " ",
    "foreground": "p:transient",
    "background": "transparent"
  },
  "blocks": [
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "leading_diamond": " ",
          "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
          "foreground": "p:purple",
          "background": "p:bg",
          "type": "git",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}{{ end }}",
            "{{ if gt .Ahead 0 }}{{ end }}",
            "{{ if gt .Behind 0 }}{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "{{.Icon}} ",
          "foreground": "p:os",
          "background": "p:bg",
          "type": "os"
        },
        {
          "style": "plain",
          "template": "{{ .UserName }}@{{ .HostName }} ",
          "foreground": "p:user",
          "background": "p:bg",
          "type": "session"
        },
        {
          "properties": {
            "folder_icon": "....",
            "home_icon": "~",
            "style": "unique"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "foreground": "p:orange",
          "background": "p:bg",
          "type": "path"
        },
        {
          "style": "plain",
          "template": "",
          "foreground": "p:closer",
          "background": "p:bg",
          "type": "text"
        }
      ]
    }
  ],
  "version": 3,
  "auto_upgrade": false,
  "final_space": true
}
